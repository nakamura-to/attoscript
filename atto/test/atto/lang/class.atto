class Parent
	name: 'foo'
	hello: { -> 'hello ' + @name }
end

assert(Parent.class == 'Parent')

class Child extends Parent
	constructor: { @name, @age -> }
	hi: { -> 'hi ' + super.name + ' ' + @age }
end

assert(Child.class == 'Child')

child = Child('hoge', 30)
result = child.hello()
print(result)
assert(result == 'hello hoge', 'hello')

result2 = child.hi()
print(result2)
assert(result2 == 'hi foo 30', 'hi')

print("done!")
